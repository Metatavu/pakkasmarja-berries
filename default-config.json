{
  "server-group": "group-1",
  "app-version": "1.2.7",
  "mode": "PRODUCTION",
  "wkhtmltopdf": {
    "command": "/opt/wkhtmltox/bin/wkhtmltopdf"
  },
  "tasks": {
    "tableName": "tasks",
    "queues": {
      "contractDocumentStatus": {
        "concurrent": 1,
        "afterProcessDelay": 5000,
        "maxTimeout": 20000
      },
      "contractDocumentStatusBatch": {
        "concurrent": 1,
        "afterProcessDelay": 5000,
        "maxTimeout": 20000
      },
      "sapContactUpdate": {
        "concurrent": 1,
        "afterProcessDelay": 1000,
        "maxTimeout": 20000
      },
      "sapDeliveryPlaceUpdate": {
        "concurrent": 1,
        "afterProcessDelay": 1000,
        "maxTimeout": 20000
      },
      "sapItemGroupUpdate": {
        "concurrent": 1,
        "afterProcessDelay": 1000,
        "maxTimeout": 20000
      },
      "sapContractUpdate": {
        "concurrent": 1,
        "afterProcessDelay": 1000,
        "maxTimeout": 20000
      }
    }
  },
  "amqp": {
    "url": "amqp://localhost"
  }
}